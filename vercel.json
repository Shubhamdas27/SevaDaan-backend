{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/server.js"
    }
  ],
  "functions": {
    "dist/server.js": {
      "runtime": "@vercel/node@3.0.0",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
